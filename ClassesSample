!procedure tableA
  class tableA <<entity>> {
    + id : T {PK}
    + attribute1 :T {required}
    + attribute2 :T
    +...
  }
!endprocedure

!procedure tableB
  class tableB <<entity>> {
    + id1 : T {PK}
    + id2 : T {PK}
    + attribute1 :T
    + attribute2 :integer {constraint = "value >0"}
    + attribute3 :integer
    +...
  }
!endprocedure

!procedure tableC 
  class tableC <<entity>> {
    + id :T {PK}
    + attribute1 :T {required}
  }
!endprocedure

!procedure tableD
  class tableD <<entity>> {
    + id :T {unique}
    + attribute1 :T {required}
  }
!endprocedure

tableA -- tableB :tableA.id = tableB.id1
tableA -- tableC :tableA.attribute2 = tableC.id
tableB -- tableD :tableB.attribute1 = tableD.id
